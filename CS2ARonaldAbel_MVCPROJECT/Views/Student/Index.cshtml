@model IEnumerable<CS2ARonaldAbel_MVCPROJECT.BusLogic.Model.tblStudent>

<div>
    <h2>Student List</h2>
    <table>
        <thead>
            <tr>
                <th>First Name</th>
                <th>LastName</th>
                <th>Age</th>
                <th>Course</th>
            </tr>
        </thead>
        <tbody>
                @foreach (var student in Model)
                {
                    <tr>
                        <td>@student.FirstName</td>
                        <td>@student.LastName</td>
                        <td>@student.Age</td>
                        <td>@student.Course</td>
                    </tr>
                }
        </tbody>
    </table>
</div>